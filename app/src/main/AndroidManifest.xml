<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="christopheinformatique.sendbill">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ClientActivity" />
        <activity android:name=".AjoutClientActivity" />
        <activity android:name=".ClientSelectionnerActivity" />
        <activity android:name=".ModifierClientActivity" />
        <activity android:name=".FactureActivity" />
        <activity android:name=".FactureClientSelectActivity" />
        <activity android:name=".ConfirmationFactureActivity" />
        <activity android:name=".ConfirmationFactureSansSignatureActivity" />
        <activity android:name=".FactureClientSelectionnerActivity"></activity>
    </application>

</manifest>